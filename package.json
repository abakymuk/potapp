{
  "name": "potlucky",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "lint": "eslint .",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "typecheck": "turbo run typecheck",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,css,scss,yml,yaml}": [
      "prettier -w"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "eslint": "^9",
    "eslint-config-prettier": "^9",
    "eslint-import-resolver-typescript": "^3",
    "eslint-plugin-import": "^2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3",
    "turbo": "^2.0.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^7"
  },
  "dependencies": {
    "posthog-js": "^1.261.0",
    "posthog-node": "^5.8.1"
  }
}
